<?xml version='1.0' encoding='UTF-8'?>
<agent-input>
  <variables>
    <variable name='sharedLocation' value='/opt/IBM/IMShared'/>
    <variable name='wctInstallLocation' value='/opt/IBM/WebSphere/Toolbox'/>
  </variables>
  <server>
    <repository location='/nas/software/IBM/WCT/FP/FP18'/>
  </server>
  <profile id='WebSphere Customization Toolbox V9.0' installLocation='${wctInstallLocation}'>
    <data key='cic.selector.arch' value='x86_64'/>
  </profile>
  <install>
    <!-- WebSphere Customization Toolbox 9.0.5.18 -->
    <offering profile='WebSphere Customization Toolbox V9.0' id='com.ibm.websphere.WCT.v90' version='9.0.5018.20231113_1345' features='core.feature,pct,zmmt,zpmt'/>
  </install>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
  <preference name='offering.service.repositories.areUsed' value='false'/>
</agent-input>