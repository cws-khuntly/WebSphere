<?xml version='1.0' encoding='UTF-8'?>
<agent-input>
  <variables>
    <variable name='sharedLocation' value='/opt/IBM/IMShared'/>
    <variable name='wasInstallLocation' value='/opt/IBM/WebSphere/AppServer'/>
  </variables>
  <server>
    <repository location='/nas/software/IBM/WAS/FP/FP18'/>
  </server>
  <profile id='IBM WebSphere Application Server V9.0' installLocation='${wasInstallLocation}'>
    <data key='cic.selector.arch' value='x86_64'/>
  </profile>
  <install>
    <!-- IBM WebSphere Application Server Network Deployment  9.0.5.18 -->
    <offering profile='IBM WebSphere Application Server V9.0' id='com.ibm.websphere.ND.v90' version='9.0.5018.20231113_1345' features='core.feature,ejbdeploy,embeddablecontainer,thinclient'/>
  </install>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
</agent-input>
