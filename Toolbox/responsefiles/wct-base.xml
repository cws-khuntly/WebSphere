<?xml version='1.0' encoding='UTF-8'?>
<agent-input>
    <variables>
        <variable name='sharedLocation' value='/opt/IBM/IMShared'/>
        <variable name='wctInstallLocation' value='/opt/IBM/WebSphere/Toolbox'/>
    </variables>
    <server>
        <repository location='/opt/installables/IBM/WCT/Base'/>
        <repository location='/opt/installables/IBM/JDK/Base'/>
    </server>
    <profile id='WebSphere Customization Toolbox V9.0' installLocation='${wctInstallLocation}'>
        <data key='cic.selector.arch' value='x86_64'/>
    </profile>
    <install>
        <!-- WebSphere Customization Toolbox 9.0.5.0 -->
        <offering profile='WebSphere Customization Toolbox V9.0' id='com.ibm.websphere.WCT.v90' version='9.0.5000.20190610_1342' features='core.feature,pct,zpmt,zmmt'/>
        <!-- IBM SDK, Java Technology Edition, Version 8 8.0.5.35 -->
        <offering profile='WebSphere Customization Toolbox V9.0' id='com.ibm.java.jdk.v8' version='8.0.5035.20190422_0948' features='com.ibm.sdk.8'/>
    </install>
    <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
    <preference name='offering.service.repositories.areUsed' value='false'/>
</agent-input>
